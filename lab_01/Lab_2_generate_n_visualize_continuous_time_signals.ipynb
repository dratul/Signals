   "source": [
    "# Function to generate unit ramp signal r(t)\n",
    "# r(t)= t for t>= 0, r(t)= 0 otherwise\n",
    "def unit_ramp(n):\n",
    "    ramp =[]\n",
    "    for sample in n:\n",
    "        if sample<0:\n",
    "            ramp.append(0)\n",
    "        else:\n",
    "            ramp.append(sample)\n",
    "    return ramp\n",
    "\n",
    "UL = 10\n",
    "LL = -10\n",
    "t = np.arange(LL, UL, 1)\n",
    "r = unit_ramp(t)\n",
    "plt.plot(t, r)\n",
    "plt.xlabel('$t$')\n",
    "plt.xticks(np.arange(LL, UL, 1))\n",
    "plt.yticks([0, UL, 1])\n",
    "plt.ylabel('r[t]')\n",
    "plt.title('Unit Ramp r[t]')\n",
    "#plt.savefig(\"UnitRamp.png\")\n",
    "plt.grid()\n"
   ]
